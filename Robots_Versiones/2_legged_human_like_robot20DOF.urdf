<?xml version="1.0"?>
<robot name="biped_robot">
  <!-- Link: Base (Wider Waist) -->
  <link name="base_link">
    <inertial>
      <mass value="8.0"/>
      <inertia ixx="0.325" ixy="0" ixz="0" iyy="0.050" iyz="0" izz="0.365"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.3 0.6 0.1"/>  <!-- Increased width (0.6 along X-axis) -->
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.3 0.6 0.1"/>  <!-- Wider waist for more stability -->
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </collision>
  </link>
  <!-- ===============================================================
                          LEFT HIP
       ===============================================================
   -->

<!-- (Opcional) Hip yaw comentado: descomenta si lo necesitas -->
  <!--
  <joint name="left_hip_yaw_joint" type="revolute">
    <parent link="base_link"/>
    <child  link="left_hip_yaw_link"/>
    <origin xyz="0 0.25 -0.05" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.5" upper="0.5" effort="120" velocity="6.0"/>
    <dynamics damping="0.1" friction="0.0"/>
  </joint>
  <link name="left_hip_yaw_link">
    <inertial><mass value="0.05"/>
      <inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4"/>
    </inertial>
  </link>
  -->

  <!-- Hip pitch -->
  <joint name="left_hip_pitch_joint" type="revolute">
    <parent link="base_link"/>
    <child link="left_hip_pitch_link"/>
    <origin xyz="0 0.25 -0.05" rpy="0 0 0"/>  <!-- Adjusted offset for wider waist -->
    <axis xyz="0 1 0"/>
    <limit lower=" -1.2" upper="1.2" effort="120" velocity="6.0"/>
    <dynamics damping="0.2" friction="0.0"/>
  </joint>

  <link name="left_hip_pitch_link">
    <inertial><mass value="0.05"/>
      <inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4"/>
    </inertial>
  </link>

  <!-- Hip Roll -->
  <joint name="left_hip_roll_joint" type="revolute">
    <parent link="left_hip_pitch_link"/>
    <child  link="left_thigh_link"/>
    <origin xyz="0 0.0 0.0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/> <!-- roll -->
    <limit lower="-0.25" upper="0.25" effort="120" velocity="6.0"/>
    <dynamics damping="0.15" friction="0.0"/>
  </joint>

  <!-- <link name="left_hip_roll_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.05"/>
      <inertia ixx="1e-4" iyy="1e-4" izz="1e-4" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link> -->

  <!-- (opcional) si quieres aislar el muslo: -->
  <!-- <joint name="left_hip_to_thigh" type="fixed">
    <parent link="left_hip_roll_link"/>
    <child  link="left_thigh_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint> -->

  

  <!-- Link: Left Thigh -->
  <link name="left_thigh_link">
    <inertial>
      <mass value="4.0"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/>
      <origin xyz="0 0 -0.3" rpy="0 0 0"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.08" length="0.6"/>
      </geometry>
      <origin xyz="0 0 -0.3" rpy="0 0 0"/>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.08" length="0.6"/>
      </geometry>
      <origin xyz="0 0 -0.3" rpy="0 0 0"/>
    </collision>
  </link>

    <!-- =======================
       LEFT Knee: pitch 
      ======================== -->

  <!-- Joint 2: Left Knee Joint -->
  <joint name="left_knee_joint" type="revolute">
    <parent link="left_thigh_link"/>
    <child link="left_shin_link"/>
    <origin xyz="0 0 -0.6" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="1.571" effort="120" velocity="6.0"/>
    <dynamics damping="0.1" friction="0.0"/>
  </joint>

  <!-- Link: Left Shin -->
  <link name="left_shin_link">
    <inertial>
      <mass value="4.0"/>
      <inertia ixx="0.004" ixy="0" ixz="0" iyy="0.004" iyz="0" izz="0.004"/>
      <origin xyz="0 0 -0.25" rpy="0 0 0"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.5"/>
      </geometry>
      <origin xyz="0 0 -0.25" rpy="0 0 0"/>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.5"/>
      </geometry>
      <origin xyz="0 0 -0.25" rpy="0 0 0"/>
    </collision>
  </link>

  <!-- =======================
       LEFT ANKLE: pitch -> roll
  ======================== -->
 <!-- left ankle pitch -->
  <joint name="left_ankle_pitch_joint" type="revolute">
    <parent link="left_shin_link"/>
    <child  link="left_ankle_pitch_link"/>
    <origin xyz="0 0 -0.5" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.5" upper="0.5" effort="60" velocity="6.0"/>
    <dynamics damping="0.1" friction="0.0"/>
  </joint>
  <link name="left_ankle_pitch_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.05"/>
      <inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4"/>
    </inertial>
  </link>

  <!-- Left Ankle Joint roll -->
  <joint name="left_ankle_roll_joint" type="revolute">
    <parent link="left_ankle_pitch_link"/>
    <child link="left_foot_link"/>
    <origin xyz="0 0 0.0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.35" upper="0.35" effort="35" velocity="6.0"/>
    <dynamics damping="0.1" friction="0.0"/>
  </joint>

  <!-- Link: Left Foot -->
  <link name="left_foot_link">
    <inertial>
      <mass value="0.5"/>  <!-- Heavier feet for stability -->
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
      <origin xyz="0 0 -0.035" rpy="0 0 0"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.3 0.15 0.05"/>  <!-- Larger foot size for more stability -->
      </geometry>
      <origin xyz="0.1 0 -0.035" rpy="0 0 0"/>
      <material name="green">
        <color rgba="0 1 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.3 0.15 0.05"/>
      </geometry>
      <origin xyz="0.1 0 -0.035" rpy="0 0 0"/>
    </collision>
  </link>

  <!--
    =====================================================
                  RIGHT HIP
    ===================================================== 
   -->

   <!-- (Opcional) Hip yaw comentado (derecha) -->
  <!--
  <joint name="right_hip_yaw_joint" type="revolute">
    <parent link="base_link"/>
    <child  link="right_hip_yaw_link"/>
    <origin xyz="0 -0.25 -0.05" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.5" upper="0.5" effort="120" velocity="6.0"/>
    <dynamics damping="0.1" friction="0.0"/>
  </joint>
  <link name="right_hip_yaw_link">
    <inertial><mass value="0.05"/>
      <inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4"/>
    </inertial>
  </link>
  -->

  <!-- Right Leg (same as left but mirrored) -->
  <joint name="right_hip_pitch_joint" type="revolute">
    <parent link="base_link"/>
    <child link="right_hip_pitch_link"/>
    <origin xyz="0 -0.25 -0.05" rpy="0 0 0"/>  
    <!-- Invierto eje para evitar transmisiÃ³n de fuerzas a lo largo de cadera -->
    <axis xyz="0 -1 0"/>
    <limit lower="-1.2" upper="1.2" effort="120" velocity="6.0"/>
    <dynamics damping="0.2" friction="0.0"/>
  </joint>

  <!-- Agregado para dar +1 DOF en el robot -->
  <link name="right_hip_pitch_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.05"/>
      <inertia ixx="1e-4" iyy="1e-4" izz="1e-4" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <joint name="right_hip_roll_joint" type="revolute">
    <parent link="right_hip_pitch_link"/>
    <child  link="right_thigh_link"/>
    <origin xyz="0 0.0 0.0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/> <!-- roll -->
    <limit lower="-0.25" upper="0.25" effort="120" velocity="6.0"/>
    <dynamics damping="0.15" friction="0.0"/>
  </joint>

  <!-- <link name="right_hip_roll_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.05"/>
      <inertia ixx="1e-4" iyy="1e-4" izz="1e-4" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link> -->

  <!-- Link: Right Thigh -->
  <link name="right_thigh_link">
    <inertial>
      <mass value="4.0"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/>
      <origin xyz="0 0 -0.3" rpy="0 0 0"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.08" length="0.6"/>
      </geometry>
      <origin xyz="0 0 -0.3" rpy="0 0 0"/>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.08" length="0.6"/>
      </geometry>
      <origin xyz="0 0 -0.3" rpy="0 0 0"/>
    </collision>
  </link>
  

  <!-- Joint 2: Right Knee Joint -->
  <joint name="right_knee_joint" type="revolute">
    <parent link="right_thigh_link"/>
    <child link="right_shin_link"/>
    <origin xyz="0 0 -0.6" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="1.571" effort="60" velocity="6.0"/>
    <dynamics damping="0.1" friction="0.0"/>
  </joint>

  <!-- Link: Right Shin -->
  <link name="right_shin_link">
    <inertial>
      <mass value="4.0"/>
      <inertia ixx="0.004" ixy="0" ixz="0" iyy="0.004" iyz="0" izz="0.004"/>
      <origin xyz="0 0 -0.25" rpy="0 0 0"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.5"/>
      </geometry>
      <origin xyz="0 0 -0.25" rpy="0 0 0"/>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.5"/>
      </geometry>
      <origin xyz="0 0 -0.25" rpy="0 0 0"/>
    </collision>
  </link>

  <!-- =======================
       RIGHT ANKLE: pitch -> roll
  ======================== -->

  <joint name="right_ankle_pitch_joint" type="revolute">
    <parent link="right_shin_link"/>
    <child  link="right_ankle_pitch_link"/>
    <origin xyz="0 0 -0.5" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.5" upper="0.5" effort="60" velocity="6.0"/>
    <dynamics damping="0.1" friction="0.0"/>
  </joint>
  <link name="right_ankle_pitch_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.05"/>
      <inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4"/>
    </inertial>
  </link>

  <!-- Joint 3: Right Ankle Joint -->
  <joint name="right_ankle_roll_joint" type="revolute">
    <parent link="right_ankle_pitch_link"/>
    <child link="right_foot_link"/>
    <origin xyz="0 0 0.0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.35" upper="0.35" effort="35" velocity="6.0"/>
    <dynamics damping="0.1" friction="0.0"/>
  </joint>

  <!-- Link: Right Foot -->
  <link name="right_foot_link">
    <inertial>
      <mass value="0.5"/>  <!-- Heavier feet for stability -->
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
      <origin xyz="0 0 -0.035" rpy="0 0 0"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.3 0.15 0.05"/>  <!-- Larger foot size for more stability -->
      </geometry>
      <origin xyz="0.1 0 -0.035" rpy="0 0 0"/>
      <material name="green">
        <color rgba="0 1 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.3 0.15 0.05"/>
      </geometry>
      <origin xyz="0.1 0 -0.035" rpy="0 0 0"/>
    </collision>
  </link>
</robot>
